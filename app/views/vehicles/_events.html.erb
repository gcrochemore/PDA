La liste des events

<table class="table table-condensed">
  <thead>
    <tr>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.date %></td>
        <td><%= event.class.name %></td>
        <% if event.class.name == 'VehicleKilometricStatement' %>
	        <td><%= event.mileage %></td>
	        <td><%= event.use_duration %></td>
	        <td></td>
	        <td></td>
        <% else %>
        	<td></td>
	        <td></td>
	        <td><%= event.andand.volume %></td>
	        <td><%= event.andand.amount %></td>
	    <% end %>
      </tr>
    <% end %>
  </tbody>
</table>