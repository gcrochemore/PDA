<%= simple_form_for(@work_experience) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :resume %>
    <%= f.input :name %>
    <%= f.input :scholl %>
    <%= f.input :begin_date %>
    <%= f.input :end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
