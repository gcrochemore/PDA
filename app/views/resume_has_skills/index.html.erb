<p id="notice"><%= notice %></p>

<h1>Resume Has Skills</h1>

<table>
  <thead>
    <tr>
      <th>Resume</th>
      <th>Skill</th>
      <th>Level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resume_has_skills.each do |resume_has_skill| %>
      <tr>
        <td><%= resume_has_skill.resume %></td>
        <td><%= resume_has_skill.skill %></td>
        <td><%= resume_has_skill.level %></td>
        <td><%= link_to 'Show', resume_has_skill %></td>
        <td><%= link_to 'Edit', edit_resume_has_skill_path(resume_has_skill) %></td>
        <td><%= link_to 'Destroy', resume_has_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resume Has Skill', new_resume_has_skill_path %>
